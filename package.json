{
  "name": "learnai-portal",
  "version": "1.0.0",
  "description": "AI-powered tutoring marketplace connecting students with expert professors",
  "author": {
    "name": "Ruslan Magana",
    "url": "https://ruslanmv.com"
  },
  "license": "MIT",
  "private": true,
  "homepage": "https://ruslanmv.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruslanmv/learnai"
  },
  "keywords": [
    "education",
    "ai",
    "tutoring",
    "marketplace",
    "nextjs",
    "typescript",
    "openai",
    "learning-platform"
  ],
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm run format:check",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed": "node prisma/seed.js",
    "postinstall": "prisma generate",
    "clean": "rm -rf .next out build dist .vercel",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@next-auth/prisma-adapter": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "next": "^15.1.4",
    "next-auth": "^4.24.10",
    "openai": "^4.73.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "tldraw": "^2.4.6",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
